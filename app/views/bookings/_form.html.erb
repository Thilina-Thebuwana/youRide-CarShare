<%= video_tag "vid-bg.mp4", autoplay: :autoplay, loop: :loop, mute: :mute, class: "video", poster: "vid-poster.png" %>

<%= form_with(model: booking, local: true) do |form| %>
  <% if booking.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(booking.errors.count, "error") %> prohibited this booking from being saved:</h2>

      <ul>
      <% booking.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

 <div class="panel-body" >
  <div class="form-group">
      <text> <label><%= form.label :car_id %></label></text>
       <div class="col-md-0">
         <div id = "car_name">
    <%= form.collection_select(:car_id, Car.where(:status => "Available"), :id, :name, {:id => 'car_name'} )%>
      </div>
  </div>
</div>

  <div class="form-group">
      <text><label> <%= form.label :start_time %></label></text>
      <div class="col-md-0">
    <%= form.time_select :start_time, id: :booking_start_time , class: "form-control" %>
      </div>
  </div>

  <div class="field">
    <text><label><%= form.label :end_time %></label></text>
       <div class="col-md-0">
    <%= form.time_select :end_time, id: :booking_end_time %>
      </div>
  </div>

  
    <%= form.submit %>

      </div>


<% end %>


